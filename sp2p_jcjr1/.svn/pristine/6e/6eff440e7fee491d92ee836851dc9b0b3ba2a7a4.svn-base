<script type="text/javascript">

var user;
user = new MUser(param);
jQuery(document).ready(function($) {
	user.login_pc_check(function(data){
		if(user.isNotBank(data)){
			if(data.status == 1){
				$("#span_no_login").hide();
				$("#span_logind").show();
			}else{
				// 头部控制
				$("#span_no_login").show();
				$("#span_logind").hide();
			}
		}else{
			alert('检查登录状态失败！');	
		}
	});
	
});
