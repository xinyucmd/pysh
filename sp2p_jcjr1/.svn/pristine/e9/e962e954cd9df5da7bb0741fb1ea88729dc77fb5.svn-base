html{-ms-text-size-adjust:100%;-webkit-text-size-adjust:100%;font:24px "Microsoft YaHei";}
ul,li,div,p,body{margin:0;padding:0; border:none;-moz-user-select: none;-khtml-user-select: none;user-select: none;text-align:left;color:#515567;}
input{-webkit-appearance:button;}
ul,li {list-style:none}
a{text-decoration:none;}
a:focus { outline: none;} 
.hide{display:none;}
.clear { clear:both; margin:0 auto; padding:0;}
.fg { padding:10px 0 0;}
.f_l {float:left;}
*{-webkit-tap-highlight-color:rgba(14,159,111,0.5);-webkit-touch-callout:none;margin:0px; padding:0px;}
body { color:#515567; height: 100%;}


.banner1{ height:135px; background:#ffdb2f url(../image/banner1.png) no-repeat center; width: auto; max-width:100%;}
.banner2{ height:246px; background:#ffdb2f url(../image/banner2.png) no-repeat center; width: auto; max-width:100%;}
/*.banner3{ height:108px; background:url(../image/banner3.png) no-repeat center; width: auto; max-width:100%;}*/
.banner4{ height:636px; background:url(../image/banner3.png) no-repeat center; width: auto; max-width:100%;}
.banner4_con{ width: auto; max-width:100%; margin:0 auto;}
.banner5{ background:#c93e6a url(../image/bottom_bg.png) repeat-y; height:100%; width:100%; padding-bottom: 20px; padding-top:50px;}
.banner5_con{padding-bottom: 10px; background-color: #A32B4A; width:90%; max-width: 100%; height: auto; margin:0 auto; color:#fff;  line-height:2.4em; padding-top:50px; margin-top: -25px;}
.banner5_con i{ color:#ffed6d; font-style:normal;}
.banner5_con a{ color:#fff; font-size: 1.3em;}

.close{opacity:1; background:url(../image/close.png) no-repeat; top:5px;right:5px;  position:absolute; z-index:10001; height:27px; width:27px;}
.plate{position:relative;width:598px;height:598px;background-image:url(../image/pan.png); margin:0 auto;}
.prize{float:right;width:360px;height:390px;font:14px/1.5 "Microsoft Yahei";}
.prize h4{margin:80px 0 20px;padding:10px;background-color:#eee;}
.prize ul{list-style-type:none;}
.prize li{margin:10px;}
#plateBtn{position:absolute;left:130px;top:130px;width:333px;height:333px;background-image:url(../image/zhen.png);text-indent:-9999px;overflow:hidden;}
#result{display:none;position:absolute;left:10%;top:60%;width:80%;height:320px;background-color:rgba(0,0,0,0.75);filter:alpha(opacity=90);}
#resultTxt{padding:80px 15px 0;color:#fff;text-align:center; font-size:2.2em;}
#resultTxt em{color:#ffea76;font-style:normal;}
#resultBtn{position:absolute;right:5px;top:5px;text-indent:-100px;overflow:hidden;font-size:1.2em;}
#resultBtn a{ display:block; padding:8px 15px;}
#resultBtn img{ max-width:100%; display:block; width:50px; height:50px;}
.theme-popover{ width:80%; position:fixed; display:none; z-index:9999; top:30%; background-color:rgba(0,0,0,0.75);filter:alpha(opacity=90); border-radius:8px; left:10%;  font-size:1.2em; padding-bottom:20px; }
.theme-popover .tibo{ height:60px; line-height:60px; padding-left:20px;color:#fff;}
.login_box{ width:530px; height:677px; position:fixed; left:30%; top:0; display:none; z-index:9999;}
.thao{ height: 300px;left: 20px;overflow: hidden;position: relative;z-index:10000;}
.bg-primary{ position:absolute;  bottom:30px;  background-color:rgb(51, 122, 183); border-radius:8px; padding:15px; left:30%; width:40%; text-align:center;}
.bg-primary { color: #fff;background-color: #337ab7;}
a.bg-primary:hover {background-color: #286090;color:#fff;}
.action{ width: 96%; margin: 0 auto; height: auto; margin-top: 40px;}
.action a{ width:30%; background:#c5375b; border:solid 1px #8d1b38; border-radius:5px; display:inline-block; margin:0 1%; text-align:center; position:relative;font-size:1.2em; height:65px;}
.action img{position:relative; top:10px; left:-5px; }
