html{-ms-text-size-adjust:100%;-webkit-text-size-adjust:100%;}
body { font-family:'Microsoft YaHei', Helvetica, Tahoma; height: 100%; color: #667279; font-size:1em; line-height: 1.25em; background:#fff; padding:0; margin:0;}
body, h1, h2, h3, h4, h5, h6, p, ul, ol, li, dl, dt, dd, a, textarea, input, button, span, em, strong, img, div { -webkit-touch-callout:none; -webkit-tap-highlight-color:rgba(0, 0, 0, 0.3)}
address, cite, dfn, em, var,b,sub,sup { font-style: normal }
code, kbd, pre, samp { font-family: courier new, courier, monospace }
h1, h2, h3, h4, h5, h6, button, input, select, textarea { font-size: 100% }
input,textarea,select{ font-family:"Microsoft YaHei",Helvetica,Arial,Verdana,sans-serif; outline:0; border:none;-webkit-appearance:button;}
fieldset, img { border: 0 }
ul, ol { list-style: none }
table { border-collapse: collapse; border-spacing: 0 }
input{ outline: 0; border:none; }
img { /*vertical-align: middle ; width:100%; height:auto; display:block;*/}
a{color:#3a3a3a;text-decoration:none;}
/*layout*/
#wrap{ overflow:hidden;margin: 0 auto; width: 640px; min-height: 100%;}
#footer { background:#FAFCFC; padding-bottom:15px; text-align:center; font-size:12px; color:#9FABB2; border-top:1px solid #E3E7E8;}
.reg{ background:#ffffff;}
.link-touch{ width:90%; margin:0 5%;}
.link-touch ul li{ width:100%; border:solid 1px #c9c9c9; height:73px; line-height:73px; overflow:hidden; border-radius:5px; margin-top:25px;}
.link-touch ul li .w1{ border:solid 1px #c9c9c9; height:55px; line-height:55px; width:240px; margin-left:0;border-radius:5px; padding-left:15px;}
.link-touch ul li .w2{ }
.link-touch ul li .w3{ margin-top: 0.6em;}
.link-touch ul li .w4{ margin-top: 0.6em;}
.txt {color: #fff; text-align:left; float:left; font-size:1.6em; padding-left:1.5em; margin-left:1em; margin-top:0.3em; width:50px; height:60px;}
.input {color: #bbbbbb;  font-size:30px; margin-left:20px; background:#fff; }
.items{ padding:0;}
/*注册*/
.reg_btn{width:90%; padding:0 5%; margin: 0 auto;}
.big_btn{ width:100%; height: 78px; background:#1d9def; line-height:78px; text-align:center; color:#fff; font-size:40px; border-radius:5px; cursor:pointer;}
.big_btn_disable{ width:100%; height: 78px; background:#1d9def; line-height:78px; text-align:center; color:#fff; font-size:40px; border-radius:5px; cursor:pointer;}
.big_btn_home{ width:90%; margin: 0 auto; height: 3.75em; line-height:40px; text-align:center; color:#fff; font-size:1.4em; border-radius:5px;}
.bp1{ background:url(/images/recommend/iphone.jpg) no-repeat; }
.bp2{ background:url(/images/recommend/password.jpg) no-repeat; }
.bp3{ background:url(/images/recommend/people.jpg) no-repeat; }
.bp5{ background:url(/images/recommend/check.jpg) no-repeat; padding:15px; margin:0 10px; cursor: pointer;}
.xieyi{ height:40px; margin:30px 0px; line-height:40px; font-size:1.4em;}
.xieyi a{ color:#fff;}
.reg_bg{ background:url(/images/recommend/banner.jpg) no-repeat; width:640px; height:430px;}
.reg_text{ text-align:center; font-size:1.8em; color:#fff; line-height:36px; font-weight:500; margin-top:25px;}
.reg_con{ width:70%; margin:0 15%;}
.reg_con ul{ padding:0;}
.reg_con ul li{ border-bottom:solid 1px #9eb5d5; height:70px; line-height:70px; padding-left:2%;}
.center_btn{ width:268px; height:70px; background:#22d6b2; border-radius:5px; border:none; color:#fff; font-size:30px;  margin-left:20px; cursor:pointer;}
.center_btn_disable{ width:268px; height:70px; background:#c0c0c0; border-radius:5px; border:none; color:#fff; font-size:30px;  margin-left:20px; cursor:pointer;}
.link-touch ul .un-agree{border: none; height: 4.5em; margin-top:0;}
.link-touch ul .un-agree span{margin-top: 0.5em; display: block; font-size: 26px;}
.link-touch ul .un-agree label{ height: 1em; position:relative; top:34px; width:15px;}
.serve{ font-size:14px; color:#fff; padding:50px 20px 80px ;}
.serve-log{ font-size:1.8em;}
.serve-log a{ background:url(/images/m/top_icon.jpg) 10px 20px no-repeat; position:absolute; left:3%;  color:#fff; padding-left:200px;}
.logo{ background:url(/images/recommend/logo.jpg) no-repeat; width:262px; height:30px; margin:15px auto 10px auto;}
.succes_reg{ background:url(/images/recommend/succes_bg.jpg) no-repeat; width:640px; height:89px; margin-top:100px;}
.download{  width:640px; height:60px; margin-top:50px; }
.download a{background:url(/images/recommend/succes_bg1.jpg) no-repeat; display:block; width:640px; height:60px;}
.phone{ background:url(/images/recommend/succes_bg2.jpg) no-repeat; width:640px; height:609px; margin:60px 0 40px 0;}

