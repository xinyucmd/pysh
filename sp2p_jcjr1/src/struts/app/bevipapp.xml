<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="bevipapp" extends="app-default" namespace="/app">
		<global-results>
			<result name="error">/500.jsp</result>
		</global-results>
		<global-exception-mappings>
			<exception-mapping result="error" exception="java.lang.Exception"></exception-mapping>
		</global-exception-mappings>
		<action name="addVipInit" class="beVipAppAction" method="addVipInit">
		</action>
		
		<action name="addVip" class="beVipAppAction" method="addVip">
		</action>	
		
		<action name="queryServiceList" class="beVipAppAction" method="queryServiceList">
		</action>	
			
	</package>
</struts>
