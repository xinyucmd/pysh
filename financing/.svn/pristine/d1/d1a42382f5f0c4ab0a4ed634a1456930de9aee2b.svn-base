package com.jiangchuanbanking.financing.domain;

import java.io.Serializable;

import com.jiangchuanbanking.base.domain.BaseEntity;
import com.jiangchuanbanking.investor.domain.Customer;

public class BankAccount extends BaseEntity implements Serializable{

	private Integer bank_account_id;
	private String bank_account_name;
	private String bank_account_no;
	private String bank_account_addr;
	private String sts;
	private String updata_time;
	private String op_no;
	private Integer open_id;	
	private Customer customer;

	public Integer getOpen_id() {
		return open_id;
	}

	public void setOpen_id(Integer open_id) {
		this.open_id = open_id;
	}

	public Customer getCustomer() {
		return customer;
	}

	public void setCustomer(Customer customer) {
		this.customer = customer;
	}

	public Integer getBank_account_id() {
		return bank_account_id;
	}

	public void setBank_account_id(Integer bank_account_id) {
		this.bank_account_id = bank_account_id;
	}

	public String getBank_account_name() {
		return bank_account_name;
	}

	public void setBank_account_name(String bank_account_name) {
		this.bank_account_name = bank_account_name;
	}

	public String getBank_account_no() {
		return bank_account_no;
	}

	public void setBank_account_no(String bank_account_no) {
		this.bank_account_no = bank_account_no;
	}

	public String getBank_account_addr() {
		return bank_account_addr;
	}

	public void setBank_account_addr(String bank_account_addr) {
		this.bank_account_addr = bank_account_addr;
	}

	public String getSts() {
		return sts;
	}

	public void setSts(String sts) {
		this.sts = sts;
	}

	public String getUpdata_time() {
		return updata_time;
	}

	public void setUpdata_time(String updata_time) {
		this.updata_time = updata_time;
	}

	public String getOp_no() {
		return op_no;
	}

	public void setOp_no(String op_no) {
		this.op_no = op_no;
	}

	
	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;

	@Override
	public String getName() {
		return this.bank_account_name;
	}

}
