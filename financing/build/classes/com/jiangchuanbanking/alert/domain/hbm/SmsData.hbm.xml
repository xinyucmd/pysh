<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-lazy="true" package="com.jiangchuanbanking.alert.domain">
  <class dynamic-insert="false" dynamic-update="false" name="SmsData" table="WEALTH_ALERTS_DATA">
   <id column="id" name="id" type="java.lang.Integer">
			<generator class="native" />
		</id>
    <property name="alert_method" column="alert_method" type="string" />
    <property name="alert_type" column="alert_type" type="string" />
    <property name="mobile" column="mobile" type="string" />
    <property name="alert_content" column="alert_content" type="string" />
    <property name="send_sts" column="send_sts" type="string" />
    <property name="create_op" column="create_op" type="string" />
    <property name="create_date" column="create_date"     type="string" />
    <property name="cmt" column="cmt" type="string" />
     <property name="cif_no" column="cif_no" type="string" />
    <property name="cif_name" column="cif_name"     type="string" />
    <property name="pact_no" column="pact_no" type="string" />
  
  </class>
</hibernate-mapping>
